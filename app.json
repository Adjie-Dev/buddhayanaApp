{
  "expo": {
    "name": "buddhayana-app",
    "slug": "buddhayana-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/buddhayana.png",
    "scheme": "buddhayana-app",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/buddhayana-app.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.wahyuaji.buddhayanaapp",
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/buddhayana.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/buddhayana-app.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d3d5d512-060d-412a-a131-a9297739bad7"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/d3d5d512-060d-412a-a131-a9297739bad7"
    }
  }
}
